{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended",
    "docker:pinDigests"
  ],
  "baseBranches": [
    "master"
  ],
  "assignees": [
    "sgoettsch"
  ],
  "useBaseBranchConfig": "merge",
  "prHourlyLimit": 0,
  "prConcurrentLimit": 0,
  "dependencyDashboard": true,
  "packageRules": [
    {
      "matchUpdateTypes": ["major", "minor"],
      "enabled": false
    }
  ]
}
